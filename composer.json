{
    "name": "novikovdvpoit/oxid-graphql",
    "description": "Projekteins OXID eSales GraphQL module",
    "type": "oxideshop-module",
    "keywords": ["oxid", "modules", "eShop", "GraphQL"],
    "homepage": "https://www.proeins.de/",
    "license": ["MIT"],
    "require": {
        "php": ">=7.4",
        "doctrine/dbal": "^v2.7",
        "oxid-esales/graphql-base": "dev-b-6.5.x"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "oxideshop": {
            "target-directory": "projekteins/pr_graphql",
            "blacklist-filter": [
                "tests/**/*"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/novikovdvpoit/oxid-graphql.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Projekteins\\GraphQL\\Mutator\\": "./source/modules/projekteins/pr_graphql",
        }
    },
    "config": {
        "allow-plugins": {
            "oxid-esales/oxideshop-unified-namespace-generator": true,
            "oxid-esales/oxideshop-composer-plugin": true
        }
    }
}
